{
    "name": "full/composer-rm-plugin",
    "description": "Removes vendor files/directories on install/update",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.1 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Full\\Composer\\": "src"
        }
    },
    "extra": {
        "class": "Full\\Composer\\RmPlugin"
    }
}
